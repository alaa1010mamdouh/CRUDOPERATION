@model EmployeeViewModel


@{
    ViewData["Title"] = "Edit";
}

<h1>Edit @Model.Name </h1>
<br />
<br />

<form asp-controller="Employee" asp-action="Edit" method="post" enctype="multipart/form-data">



    <partial name="PartialViews/CreateEditPartialView" />


    <input type="submit" value="Save" class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-light">Back To List</a>
</form>
@section ValidationScriptSection {
    <partial name="_ValidationScriptsPartial" />
}

